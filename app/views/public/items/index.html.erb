<h1>商品一覧ページ</h1>
<%@itemes.each do |item| %>
  <% if customer_signed_in? %>
    <%= link_to item_path(item.id) do %>
      <%= image_tag item.get_item_image(150,150) %>
    <% end %>

    <% else %>
     <%= image_tag item.get_item_image(150,150) %>
    <% end %>
      <%= item.name %><br>
      <%=item.price %>
<%end%>